{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "noImplicitAny": true,
      "sourceMap": true,
      "declaration": true,
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
        "*": ["node_modules/*"],
      }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  